
#include <Motif.tmpl>
#include <Motif.rules>

TARGET = editor
SRCS = editor.cpp mainwindow.cpp quitdialog.cpp colors.cpp lines.cpp
OBJS = editor.o mainwindow.o quitdialog.o colors.o lines.o

CC=g++
CCOPTIONS=-O -Wall -fno-builtin
CDEBUGFLAGS=-g

LOCAL_LIBRARIES = XmClientLibs

MComplexProgramTarget($(TARGET), $(LOCAL_LIBRARIES), NullParameter)

clean::
	$(RM) Makefile

