
#include <Motif.tmpl>
#include <Motif.rules>

TARGET = editor
SRCS = editor.c mainwindow.c
OBJS = editor.o mainwindow.o

CC=g++
CCOPTIONS=-O -Wall -fno-builtin
CDEBUGFLAGS=-g

LOCAL_LIBRARIES = XmClientLibs

MComplexProgramTarget($(TARGET), $(LOCAL_LIBRARIES), NullParameter)

clean::
	$(RM) Makefile

